logloss<-function(pred,actual){
minvalue<-0.00000000000001
newpred<-pmax(pmin(pred,1-minvalue),minvalue)
logloss<--(1/length(pred))*sum(actual*log(newpred)+(1-actual)*log(1-newpred))
return(logloss)
}
